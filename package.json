{
	"name": "cypress-vite",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port 4800",
		"cy:open": "npx cypress open  --component --browser chrome ",
		"cypress": "start-server-and-test dev http://localhost:4800  'cypress open --browser chrome'",
		"test:component": "start-server-and-test dev http://localhost:4800 'cypress run --component --browser chrome'",
