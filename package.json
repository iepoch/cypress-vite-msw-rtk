{
  "name": "cypress-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "cypress": "start-server-and-test dev http://localhost:3000  'cypress open'",
    "test:component": "cypress run --browser chrome --component",
    "test:e2e": "start-server-and-test dev http://localhost:3000 'cypress run --browser chrome --e2e'",
    "format": "prettier --write src",
    "lint:fix": "biome check --apply ./src",
    "test:ci": "cypress run",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/joy": "^5.0.0-beta.28",
    "@mui/material": "^5.12.2",
    "@reduxjs/toolkit": "2.2.1",
    "@tanstack/react-router": "1.18.3",
    "@testing-library/user-event": "14.5.2",
    "dotenv": "16.4.5",
    "preline": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router": "^6.21.3",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^10.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@cypress/code-coverage": "^3.12.19",
    "@cypress/vite-plugin-cypress-esm": "^1.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/router-devtools": "^1.15.22",
    "@tanstack/router-vite-plugin": "^1.15.22",
    "@testing-library/cypress": "^10.0.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__cypress": "^5.0.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.6",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "msw": "^2.2.2",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^2.0.3",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
